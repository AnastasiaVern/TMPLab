// Queue.cpp: определяет точку входа для консольного приложения.
//

#include "stdafx.h"
#include <iostream>
#include <string>
using namespace std;
struct Queue_Item
{
	int x;
	Queue_Item* Next;
}; // элемент очереди. Очередь представлена двусвязным списком
struct Queue
{
	Queue_Item* Front;
	Queue_Item* Back;
}; //заголовок очереди
   /*! Push(int _x, Queue &q)
   * Функция добавляет элемент в очередь
   */
void Push(int _x, Queue &q) 
{
	Queue_Item *Q= new Queue_Item;
	Q->x = _x;
	Q->Next = 0;
	if (q.Front==0)
	{
		q.Front = Q;
		q.Back = Q;
	}
	q.Back->Next = Q;
	q.Back = Q;
	cout << "The element was added" << endl;

}
/*! Pop(Queue &q) 
* Функция удаляет элемент из очереди
*/
void Pop(Queue &q) 
{
	q.Front = q.Front->Next;
	cout << "The element was deleted" << endl;
}
/*! Pop(Queue &q)
* Функция выводит значение элемента очереди
*/
void Peek(Queue &q)
{
	cout << q.Front->x<<endl;
}

int main()
{
	int n,x;
	string str;
	Queue q;
	q.Front = 0;
	cout << "N" << ">>";
	cin >> n;
	cout << endl;
	for (int i = 0; i < n; i++)
	{
		cin >> str;
		if (str == "add")
		{
			cout << ">>";
			cin >> x;
			Push(x, q);
		}
		if (str == "del")
		{
			Pop(q);
		}
		if (str == "get")
		{
			Peek(q);
		}
	}
    return 0;
}

